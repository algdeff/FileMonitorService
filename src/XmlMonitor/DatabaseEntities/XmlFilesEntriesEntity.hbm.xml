<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="XmlMonitor.DatabaseEntities.XmlFilesEntriesEntity" table="xml_files_entries" schema="org">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="filename">
            <column name="filename" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="entryId">
            <column name="entry_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="entryContent">
            <column name="entry_content" sql-type="varchar(1000)" length="1000" not-null="true"/>
        </property>
        <property name="entryCreationDate">
            <column name="entry_creation_date" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>